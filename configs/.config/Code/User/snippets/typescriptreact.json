{
  // Place your snippets for typescriptreact here. Each snippet is defined under a snippet name and has a prefix, body and
  // description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
  // $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
  // same ids are connected.
  // Example:
  // "Print to console": {
  // 	"prefix": "log",
  // 	"body": [
  // 		"console.log('$1');",
  // 		"$2"
  // 	],
  // 	"description": "Log output to console"
  // }
  //
  // Typescript
  "console.log with variable name": {
    "prefix": "clgv",
    "body": ["console.log('$1', $1);"],
    "description": "import style module"
  },
  "back tick": {
    "prefix": "bt",
    "body": ["`${$1}`"],
    "description": "back tick short-hand"
  },
  // React
  "Use State": {
    "prefix": ["rhus", "us"],
    "body": [
      "const [${1:variable}, set${1/(.*)/${1:/capitalize}/}] = useState(${2:initialValue});"
    ],
    "description": ""
  },
  "style import": {
    "prefix": "simp",
    "body": ["import styles from \"./${TM_FILENAME_BASE}.module.css\";"],
    "description": "import style module"
  },
  "style props": {
    "prefix": "spro",
    "body": ["style={styles.$1}"],
    "description": "set style"
  },
  "component props interface": {
    "prefix": "cpro",
    "body": [
      "interface ${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}$1Props {$2}"
    ],
    "description": "import style module"
  },
  "reactFunctionalComponent": {
    "prefix": "rfc",
    "body": [
      "export default function ${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}$1() {",
      "  return (",
      "    <div>${TM_FILENAME_BASE}$4</div>",
      "  )",
      "}"
    ]
  },
  "reactFunctionalComponentWithProps": {
    "prefix": "rfcp",
    "body": [
      "interface ${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}$1Props {$2}",
      "",
      "export default function ${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}$1({$3}: ${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}$1Props) {",
      "  return (",
      "    <div>${TM_FILENAME_BASE}$4</div>",
      "  )",
      "}"
    ]
  },
  // React Native
  "stylesheet import": {
    "prefix": "ssimp",
    "body": ["const styles = StyleSheet.create({", "  $1: {$2},", "});"]
  },
  "reactNativeFunctionalComponentWithPropsAndStyles": {
    "prefix": "rnfcps",
    "body": [
      "import { StyleSheet, View } from 'react-native'",
      "",
      "interface ${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}$1Props {$2}",
      "",
      "export default function ${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}$1({$3}: ${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}$1Props) {",
      "  return (",
      "    <View>$4</View>",
      "  )",
      "}",
      "",
      "const styles = StyleSheet.create({});"
    ]
  }
}
