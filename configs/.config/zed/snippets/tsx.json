{
  // React
    "style import": {
      "prefix": "simp",
      "body": ["import styles from \"./${TM_FILENAME_BASE}.module.css\";"],
      "description": "import style module"
    },
    "style props": {
      "prefix": "spro",
      "body": ["style={styles.$1}"],
      "description": "import style module"
    },
    "print": {
      "prefix": "print",
      "body": ["console.log('$1', $1);"],
      "description": "import style module"
    },
    "Use State": {
      "prefix": ["rhus", "us"],
      "body": [
        "const [${1:variable}, set${1/(.*)/${1:/capitalize}/}] = useState(${2:initialValue});"
      ],
      "description": ""
    },
    // React Native
    "stylesheet import": {
      "prefix": "ssimp",
      "body": ["const styles = StyleSheet.create({$1});"]
    },
    "reactNativeFunctionalComponentWithPropsAndStyles": {
      "prefix": "rnfps",
      "body": [
        "import { StyleSheet, View } from 'react-native'",
        "",
        "interface ${TM_FILENAME_BASE}$1Props {$2}",
        "",
        "export default function ${TM_FILENAME_BASE}$1({$3}: ${TM_FILENAME_BASE}$1Props) {",
        "  return (",
        "    <View>$4</View>",
        "  )",
        "}",
        "",
        "const styles = StyleSheet.create({});"
      ]
    }
}